# Create the file `.env` in the `dist/data` folder

# Required SQL variables
SQL_HOST="mysql:host=SERVER;dbname=netmatters;port=PORT" # "netmatters" is the default database name - change if necessary
SQL_USER="ADMIN" # Ensure user has full access/edit priveleges to the database
SQL_PASS="PASSWORD"

# Optional error reporting variables
ERROR_REPORTING=32767 # Requires the int value for error_reporting(). 32767 === E_ALL | 0 === no reporting (production build)
DISPLAY_ERRORS=1
HTML_ERRORS=1